{
  "name": "@portone/server-sdk",
  "type": "module",
  "version": "0.16.0",
  "description": "PortOne JavaScript SDK for server-side usage",
  "keywords": [
    "PortOne",
    "payment",
    "포트원",
    "결제 연동"
  ],
  "author": {
    "name": "portone-dx",
    "email": "support@portone.io"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./src/index.ts",
    "./webhook": "./src/webhook.ts",
    "./auth": "./src/generated/auth/index.ts",
    "./b2b": "./src/generated/b2b/index.ts",
    "./b2b/taxInvoice": "./src/generated/b2b/taxInvoice/index.ts",
    "./common": "./src/generated/common/index.ts",
    "./identityVerification": "./src/generated/identityVerification/index.ts",
    "./payment": "./src/generated/payment/index.ts",
    "./payment/billingKey": "./src/generated/payment/billingKey/index.ts",
    "./payment/cashReceipt": "./src/generated/payment/cashReceipt/index.ts",
    "./payment/paymentSchedule": "./src/generated/payment/paymentSchedule/index.ts",
    "./payment/promotion": "./src/generated/payment/promotion/index.ts",
    "./pgSpecific": "./src/generated/pgSpecific/index.ts",
    "./platform": "./src/generated/platform/index.ts",
    "./platform/account": "./src/generated/platform/account/index.ts",
    "./platform/accountTransfer": "./src/generated/platform/accountTransfer/index.ts",
    "./platform/bulkAccountTransfer": "./src/generated/platform/bulkAccountTransfer/index.ts",
    "./platform/bulkPayout": "./src/generated/platform/bulkPayout/index.ts",
    "./platform/company": "./src/generated/platform/company/index.ts",
    "./platform/partner": "./src/generated/platform/partner/index.ts",
    "./platform/partnerSettlement": "./src/generated/platform/partnerSettlement/index.ts",
    "./platform/payout": "./src/generated/platform/payout/index.ts",
    "./platform/policy": "./src/generated/platform/policy/index.ts",
    "./platform/transfer": "./src/generated/platform/transfer/index.ts"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "require": "./dist/index.cjs",
        "import": "./dist/index.mjs"
      },
      "./webhook": {
        "types": "./dist/webhook.d.ts",
        "require": "./dist/webhook.cjs",
        "import": "./dist/webhook.mjs"
      },
      "./auth": {
        "types": "./dist/generated/auth/index.d.ts",
        "require": "./dist/generated/auth/index.cjs",
        "import": "./dist/generated/auth/index.mjs"
      },
      "./b2b": {
        "types": "./dist/generated/b2b/index.d.ts",
        "require": "./dist/generated/b2b/index.cjs",
        "import": "./dist/generated/b2b/index.mjs"
      },
      "./b2b/taxInvoice": {
        "types": "./dist/generated/b2b/taxInvoice/index.d.ts",
        "require": "./dist/generated/b2b/taxInvoice/index.cjs",
        "import": "./dist/generated/b2b/taxInvoice/index.mjs"
      },
      "./common": {
        "types": "./dist/generated/common/index.d.ts",
        "require": "./dist/generated/common/index.cjs",
        "import": "./dist/generated/common/index.mjs"
      },
      "./identityVerification": {
        "types": "./dist/generated/identityVerification/index.d.ts",
        "require": "./dist/generated/identityVerification/index.cjs",
        "import": "./dist/generated/identityVerification/index.mjs"
      },
      "./payment": {
        "types": "./dist/generated/payment/index.d.ts",
        "require": "./dist/generated/payment/index.cjs",
        "import": "./dist/generated/payment/index.mjs"
      },
      "./payment/billingKey": {
        "types": "./dist/generated/payment/billingKey/index.d.ts",
        "require": "./dist/generated/payment/billingKey/index.cjs",
        "import": "./dist/generated/payment/billingKey/index.mjs"
      },
      "./payment/cashReceipt": {
        "types": "./dist/generated/payment/cashReceipt/index.d.ts",
        "require": "./dist/generated/payment/cashReceipt/index.cjs",
        "import": "./dist/generated/payment/cashReceipt/index.mjs"
      },
      "./payment/paymentSchedule": {
        "types": "./dist/generated/payment/paymentSchedule/index.d.ts",
        "require": "./dist/generated/payment/paymentSchedule/index.cjs",
        "import": "./dist/generated/payment/paymentSchedule/index.mjs"
      },
      "./payment/promotion": {
        "types": "./dist/generated/payment/promotion/index.d.ts",
        "require": "./dist/generated/payment/promotion/index.cjs",
        "import": "./dist/generated/payment/promotion/index.mjs"
      },
      "./pgSpecific": {
        "types": "./dist/generated/pgSpecific/index.d.ts",
        "require": "./dist/generated/pgSpecific/index.cjs",
        "import": "./dist/generated/pgSpecific/index.mjs"
      },
      "./platform": {
        "types": "./dist/generated/platform/index.d.ts",
        "require": "./dist/generated/platform/index.cjs",
        "import": "./dist/generated/platform/index.mjs"
      },
      "./platform/account": {
        "types": "./dist/generated/platform/account/index.d.ts",
        "require": "./dist/generated/platform/account/index.cjs",
        "import": "./dist/generated/platform/account/index.mjs"
      },
      "./platform/accountTransfer": {
        "types": "./dist/generated/platform/accountTransfer/index.d.ts",
        "require": "./dist/generated/platform/accountTransfer/index.cjs",
        "import": "./dist/generated/platform/accountTransfer/index.mjs"
      },
      "./platform/bulkAccountTransfer": {
        "types": "./dist/generated/platform/bulkAccountTransfer/index.d.ts",
        "require": "./dist/generated/platform/bulkAccountTransfer/index.cjs",
        "import": "./dist/generated/platform/bulkAccountTransfer/index.mjs"
      },
      "./platform/bulkPayout": {
        "types": "./dist/generated/platform/bulkPayout/index.d.ts",
        "require": "./dist/generated/platform/bulkPayout/index.cjs",
        "import": "./dist/generated/platform/bulkPayout/index.mjs"
      },
      "./platform/company": {
        "types": "./dist/generated/platform/company/index.d.ts",
        "require": "./dist/generated/platform/company/index.cjs",
        "import": "./dist/generated/platform/company/index.mjs"
      },
      "./platform/partner": {
        "types": "./dist/generated/platform/partner/index.d.ts",
        "require": "./dist/generated/platform/partner/index.cjs",
        "import": "./dist/generated/platform/partner/index.mjs"
      },
      "./platform/partnerSettlement": {
        "types": "./dist/generated/platform/partnerSettlement/index.d.ts",
        "require": "./dist/generated/platform/partnerSettlement/index.cjs",
        "import": "./dist/generated/platform/partnerSettlement/index.mjs"
      },
      "./platform/payout": {
        "types": "./dist/generated/platform/payout/index.d.ts",
        "require": "./dist/generated/platform/payout/index.cjs",
        "import": "./dist/generated/platform/payout/index.mjs"
      },
      "./platform/policy": {
        "types": "./dist/generated/platform/policy/index.d.ts",
        "require": "./dist/generated/platform/policy/index.cjs",
        "import": "./dist/generated/platform/policy/index.mjs"
      },
      "./platform/transfer": {
        "types": "./dist/generated/platform/transfer/index.d.ts",
        "require": "./dist/generated/platform/transfer/index.cjs",
        "import": "./dist/generated/platform/transfer/index.mjs"
      }
    }
  },
  "scripts": {
    "format": "biome format --write .",
    "build": "unbuild",
    "test:dev": "vitest --ui --typecheck",
    "test": "vitest run --typecheck",
    "prepublish": "git tag \"js-v$(jq -r .version package.json)\" && git push --tags && pnpm build && sed -i \"s/VERSION_PLACEHOLDER/$(jq -r .version package.json)/\" jsr.json",
    "publish": "jsr publish --allow-dirty"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.3",
    "@changesets/cli": "^2.29.6",
    "@expo/spawn-async": "^1.7.2",
    "@types/node": "^24.3.1",
    "@vitest/ui": "^3.1.4",
    "jsr": "^0.13.5",
    "typedoc": "^0.28.12",
    "typescript": "^5.9.2",
    "unbuild": "^3.6.0",
    "vitest": "^3.1.4"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "license": "(Apache-2.0 OR MIT)",
  "packageManager": "pnpm@10.2.0"
}